# データベース設定（SQLite共通）
DB_DIALECT=sqlite
DB_STORAGE=./data/production.sqlite

# 環境設定
NODE_ENV=production

# JWT設定
JWT_SECRET=your-secret-key-here
JWT_EXPIRES_IN=7d

# ポート設定
PORT=5000

# フロントエンドURL
FRONTEND_URL=http://localhost:3000

# レート制限設定
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# ログレベル
LOG_LEVEL=info